{
  "name": "file-share",
  "version": "1.0.0",
  "description": "FileShare app using ES6 and React",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets env,stage-0\"",
    "build": "babel src -s -D -d dist --presets env,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "archiver": "^5.0.0",
    "aws-sdk": "^2.723.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.2",
    "chalk": "^4.1.0",
    "cors": "^2.8.4",
    "express": "^4.17.3",
    "lodash": "^4.17.19",
    "mongodb": "^3.5.9",
    "morgan": "^1.9.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.4.10"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.9.0",
    "nodemon": "^1.12.1"
  }
}
